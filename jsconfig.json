{
  "compilerOptions": {
    "target": "ES6",
    "jsx": "react",
    "baseUrl": "./src",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": false,
    "paths": {
      "Config/*": ["src/config/*"],
      "Components/*": ["src/components/*"],
      "App/*": ["src/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
